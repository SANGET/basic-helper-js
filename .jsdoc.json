{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "includePattern": "./src",
    "excludePattern": "./src/libs"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "systemName": "Basic-Helper",
    "systemSummary": "基础 js 函数库，输入类型验证等",
    "systemColor": "#5ab99b",
    "copyright": "Basic-helper",
    "cleverLinks": false,
    "inlineNav": true,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./jsdocs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/foodoc/template"
  }
}