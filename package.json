{
  "name": "basic-helper",
  "version": "1.4.0",
  "description": "",
  "scripts": {
    "publish": "npm publish ./build",
    "release": "npm run build; npm run publish",
    "build": "rm -rf ./build/; npm run build:babel; npm run build:copy-files",
    "build:babel": "NODE_ENV=production babel ./src --out-dir ./build --ignore spec.js",
    "build:copy-files": "NODE_ENV=production babel-node ./scripts/copy-files.js",
    "push": "npm publish; git add -A; git commit -m 'update'; git push origin master",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SANGET/basic-helper-js.git"
  },
  "author": "sanget",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SANGET/basic-helper-js/issues"
  },
  "homepage": "https://github.com/SANGET/basic-helper-js#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "babel-jest": "^23.6.0",
    "jest": "^23.6.0",
    "regenerator-runtime": "^0.12.1"
  }
}
